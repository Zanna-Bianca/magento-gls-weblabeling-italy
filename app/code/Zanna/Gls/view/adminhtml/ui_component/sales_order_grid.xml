<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:framework:Ui/etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="zanna_gls">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">zanna_gls</item>
                        <item name="label" xsi:type="string" translate="true">Esporta GLS</item>
                        <item name="url" xsi:type="url" path="zannagls/order/export"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>

